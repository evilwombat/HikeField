<resources>
    <bitmap id="LauncherIcon" filename="images/launcher_icon.png" />
    
    <font id="roboto_20" filename="font/roboto_20.fnt" antialias="true" />
    
    <fitContributions>
        <fitField id="0" displayInChart="false" displayInActivitySummary="true" sortOrder="0" precision="0"
        dataLabel="@Strings.steps_label" unitLabel="@Strings.steps_unit" />
        <fitField id="1" displayInChart="false" displayInActivityLaps="true" sortOrder="1" precision="0"
        dataLabel="@Strings.steps_label" unitLabel="@Strings.steps_unit" />
    </fitContributions>
    
    <properties>
        <property id="uuid" type="string">"None"</property>
        <property id="unlockCode" type="string">"unclock code"</property>
        <property id="showCadence" type="boolean">false</property>
        <property id="showHR" type="boolean">true</property>
        <property id="showMaxElevation" type="boolean">true</property>
        <property id="showNotification" type="boolean">false</property>
    </properties>
    
    <settings>
	    <setting propertyKey="@Properties.uuid" title="@Strings.uuidTitle">
	        <settingConfig type="alphaNumeric" readonly="true" />
	    </setting>
	    
	    <setting propertyKey="@Properties.unlockCode" title="@Strings.unlockCode">
	        <settingConfig type="alphaNumeric" />
	    </setting>
	    
	    <setting propertyKey="@Properties.showCadence" title="@Strings.showCadenceTitle">
	        <settingConfig type="boolean" />
	    </setting>
	    
	    <setting propertyKey="@Properties.showHR" title="@Strings.showHRTitle">
	        <settingConfig type="boolean" />
	    </setting>
	    
	    <setting propertyKey="@Properties.showMaxElevation" title="@Strings.showMaxElevationTitle">
	        <settingConfig type="boolean" />
	    </setting>
	    
	    <setting propertyKey="@Properties.showNotification" title="@Strings.showNotificationTitle">
	        <settingConfig type="boolean" />
	    </setting>
    </settings>
</resources>