<resources>
    <bitmap id="LauncherIcon" filename="images/launcher_icon.png" />
    
    <font id="roboto_20" filename="font/roboto_20.fnt" antialias="true" />
    
    <fitContributions>
        <fitField id="0" displayInChart="false" displayInActivitySummary="true" sortOrder="0" precision="0"
        dataLabel="@Strings.steps_label" unitLabel="@Strings.steps_unit" />
        <fitField id="1" displayInChart="false" displayInActivityLaps="true" sortOrder="1" precision="0"
        dataLabel="@Strings.steps_label" unitLabel="@Strings.steps_unit" />
    </fitContributions>
    
    <properties>
        <property id="showCadence" type="boolean">true</property>
        <property id="uuid" type="string">"None"</property>
    </properties>
    
    <settings>
	    <setting propertyKey="@Properties.showCadence" title="@Strings.showCadenceTitle">
	        <settingConfig type="boolean" />
	    </setting>
	    
	    <setting propertyKey="@Properties.uuid" title="@Strings.uuidTitle">
	        <settingConfig type="alphaNumeric" readonly="true" />
	    </setting>
    </settings>
</resources>